{
    "name": "inszenium/contao-usercentrics-bundle",
    "description": "Integration von Usercentrics",
    "keywords": [
        "contao",
        "bundle",
        "usercentrics"
    ],
    "type": "contao-bundle",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Kirsten Roschanski",
            "email": "support@inszenium.de",
            "homepage": "https://github.com/inszenium/contao-usercentrics-bundle",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/inszenium/contao-usercentrics-bundle/issues",
        "source": "https://github.com/inszenium/contao-usercentrics-bundle"
    },
    "require": {
        "php": "^8.0",
        "contao/core-bundle": "^4.13 || ^5.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.3.1",
        "contao/test-case": "^4.5",
        "phpunit/phpunit": "^9.3",
        "symfony/http-client": "5.4.*",
        "symfony/phpunit-bridge": "^5.4",
        "contao/easy-coding-standard": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Inszenium\\ContaoUsercentricsBundle\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "contao-components/installer": true,
            "contao/manager-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "contao-community-alliance/composer-plugin": true
        }
    },
    "extra": {
        "contao-manager-plugin": "Inszenium\\ContaoUsercentricsBundle\\ContaoManager\\Plugin"
    }
}
